<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="20.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane hgap="5.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" vgap="5.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" prefWidth="150.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints vgrow="NEVER" />
            <RowConstraints vgrow="NEVER" />
            <RowConstraints vgrow="NEVER" />
            <RowConstraints vgrow="NEVER" />
         </rowConstraints>
         <children>
            <Label text="Naive approach:" />
            <Label text="Client predict:" GridPane.rowIndex="1" />
            <Label text="Client smoothing:" GridPane.rowIndex="2" />
            <Label text="Client smoothing factor:" GridPane.rowIndex="3" />
            <CheckBox fx:id="inputNaive" mnemonicParsing="false" GridPane.columnIndex="1" />
            <CheckBox fx:id="inputPredict" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <CheckBox fx:id="inputSmoothing" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Spinner fx:id="inputSmoothingFactor" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         </children>
      </GridPane>
      <GridPane hgap="5.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" vgap="5.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" prefWidth="150.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints vgrow="NEVER" />
            <RowConstraints vgrow="NEVER" />
         </rowConstraints>
         <children>
            <Label text="Show server position:" />
            <Label text="Show dest position:" GridPane.rowIndex="1" />
            <CheckBox fx:id="inputServerPosition" mnemonicParsing="false" GridPane.columnIndex="1" />
            <CheckBox fx:id="inputDestination" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <GridPane hgap="5.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" vgap="5.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" prefWidth="150.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints vgrow="NEVER" />
            <RowConstraints vgrow="NEVER" />
         </rowConstraints>
         <children>
            <Label text="Net offset:" />
            <Label text="Net fake lag:" GridPane.rowIndex="1" />
            <Spinner fx:id="inputOffset" GridPane.columnIndex="1" />
            <Spinner fx:id="inputLag" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
